{
  "moduleNameMapper": {
    "\\.((s)css|less)$": "identity-obj-proxy"
  },
  "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
  "testURL": "http://localhost/",
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  },
  "coverageReporters": [
    "json",
    "lcovonly",
    "text",
    "clover"
  ]
}
